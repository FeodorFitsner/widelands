Import('env')
from Distribute import distadd

distadd(env, 'build/SConscript')
distadd(env, 'build/build1.sh')
distadd(env, 'build/build.txt')
distadd(env, 'build/build-with-cross.sh')
distadd(env, 'build/debian-crossbuild')
distadd(env, 'build/scons-config.py')
distadd(env, 'build/win32')
distadd(env, 'build/win32-vctoolkit.bat')

Export('env')
SConscript('scons-tools/SConscript')
