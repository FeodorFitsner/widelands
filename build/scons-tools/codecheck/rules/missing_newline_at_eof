#!/usr/bin/python -tt

class EvalMatches( object ):
    def __call__(self, lines,fn):
        errors = []
        if lines[-1][-1] != '\n':
                errors.append( (fn, len(lines), "Missing newline at eof!"))
        return errors


evaluate_matches = EvalMatches()

allowed = [
    "jlhsdfjh\n",
    "ksdhdsf\nkjshfkjhdsf\n"
]
forbidden = [
    "jlhsdfjh",
    "ksdhdsf\nkjshfkjhdsf"
]
