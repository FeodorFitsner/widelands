#: ../tribes/barbarians/workers/master-brewer/conf:2
msgid "Master Brewer"
msgstr "Mistrz piwowarów"

#: ../tribes/barbarians/workers/master-miner/conf:2
msgid "Master Miner"
msgstr "Mistrz górników"

#: ../tribes/barbarians/workers/miner/conf:2
msgid "Miner"
