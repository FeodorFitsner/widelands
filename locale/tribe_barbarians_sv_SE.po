msgid "Master Blacksmith"
msgstr "Storsmed"

#: ../tribes/barbarians/workers/master-brewer/conf:2
msgid "Master Brewer"
msgstr "Storbryggare"

#: ../tribes/barbarians/workers/master-miner/conf:2
msgid "Master Miner"
msgstr "Gruvchef"

