#: ../tribes/barbarians/workers/stonemason/conf:2
msgid "Stonemason"
msgstr "Stenhuggare"

#: ../tribes/barbarians/workers/trainer/conf:2
msgid "Trainer"
msgstr "Tränare"

#: ../tribes/barbarians/workers/weaver/conf:2
msgid "Weaver"
msgstr "Skräddare"
