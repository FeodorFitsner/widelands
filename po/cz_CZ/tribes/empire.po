#: ../../../tribes/empire/workers/innkeeper/conf:2
msgid "Innkeeper"
msgstr "Hospodský"

#: ../../../tribes/empire/workers/lumberjack/conf:2
msgid "Lumberjack"
msgstr "Dřevorubec"

#: ../../../tribes/empire/workers/master-miner/conf:2
msgid "Master Miner"
msgstr "Mistr horník"

#: ../../../tribes/empire/workers/miller/conf:2
msgid "Miller"
msgstr "Mlynář"

#: ../../../tribes/empire/workers/miner/conf:2
