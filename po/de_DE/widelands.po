#: ../../src/wlapplication.cc:1016
msgid ""
" --<config-entry-name>=value overwrites any config file setting\n"
"\n"
" --record=FILENAME    Record all events to the given filename for later playback\n"
" --playback=FILENAME  Playback given filename (see --record)\n"
"\n"
" --coredump=[yes|no]  Generates a core dump on segfaults instead of using the SDL\n"
msgstr ""
" --<Konfigschlüssel>=Wert Definiert beliebige Werte der Konfigurationsdatei\n"
"\n"
" --record=DATEINAME   Protokolliere alle Ereignisse in der angegebenen Datei.\n"
"                      Die Datei kann später mit --playback abgespielt werden.\n"
" --playback=DATEINAME Datei abspielen (die mit --record erzeugt wurde).\n"
"\n"
" --coredump=[yes|no]  Erzeugt ein Speicherabbild bei Segmentfehlern.\n"
"                      Schaltet den Mechanismus von SDL ab.\n"

#: ../../src/wlapplication.cc:1022
msgid " --ggz                Starts game as GGZ Gaming Zone client (don't use!)"
msgstr " --ggz                Started das Spiel als GGZ Gaming Zone Klienten (nicht benutzen!)"

#: ../../src/wlapplication.cc:1025
msgid ""
" --nosound            Starts the game with sound disabled\n"
" --nozip              Do not save files as binary zip archives.\n"
"\n"
" --editor             Directly starts the Widelands editor.\n"
msgstr ""
" --nosound            Startet das Spiel ohne Geräuschwiedergabe.\n"
" --nozip              Dateien nicht komprimiert abspeichern.\n"
"\n"
" --editor             Startet direkt den Widelands Karten Editor.\n"

#: ../../src/wlapplication.cc:1031
msgid " --double             Start the game twice (for localhost network testing)\n"
msgstr ""
" --double             Startet das Spiel doppelt\n"
"                      Für Tests über das localhost Netz.\n"

#: ../../src/wlapplication.cc:1035
msgid " --help               Show this help\n"
msgstr " --help               Zeigt diese Hilfe\n"

