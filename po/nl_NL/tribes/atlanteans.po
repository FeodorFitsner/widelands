msgstr "Ereprijskweker"

#: ../../../tribes/atlantids/workers/builder/conf:2
msgid "Builder"
msgstr "Bouwvakker"

#: ../../../tribes/atlantids/workers/burner/conf:2
msgid "Burner"
msgstr "Brander"

#: ../../../tribes/atlantids/workers/carrier/conf:2
msgid "Carrier"
msgstr "Drager"

#: ../../../tribes/atlantids/workers/farmer/conf:2
msgid "Farmer"
msgstr "Boer"

#: ../../../tribes/atlantids/workers/fish_breeder/conf:2
msgid "Fish Breeder"
msgstr "Viskweker"

#: ../../../tribes/atlantids/workers/fish_breeder/conf:3
msgid "Breeds fish"
msgstr "Kweekt vis"

#: ../../../tribes/atlantids/workers/fisher/conf:2
msgid "Fisher"
msgstr "Visser"

#: ../../../tribes/atlantids/workers/fisher/conf:3
msgid "The fisher fishes delicious fish."
msgstr "De visser vangt heerlijke vis."

#: ../../../tribes/atlantids/workers/forester/conf:2
msgid "Forester"
msgstr "Boswachter"

#: ../../../tribes/atlantids/workers/forester/conf:3
msgid "Plants trees."
msgstr "Plant bomen."

#: ../../../tribes/atlantids/workers/geologist/conf:2
msgid "Geologist"
msgstr "Geoloog"

#: ../../../tribes/atlantids/workers/hunter/conf:2
