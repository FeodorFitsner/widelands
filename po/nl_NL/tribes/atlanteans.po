msgstr "Jager"

#: ../../../tribes/atlantids/workers/miller/conf:2
msgid "Miller"
msgstr "Molenaar"

#: ../../../tribes/atlantids/workers/miner/conf:2
msgid "Miner"
msgstr "Mijnwerker"

#: ../../../tribes/atlantids/workers/sawyer/conf:2
msgid "Sawyer"
msgstr "Zager"

#: ../../../tribes/atlantids/workers/smelter/conf:2
msgid "Smelter"
msgstr "Smelter"

#: ../../../tribes/atlantids/workers/smoker/conf:2
msgid "Smoker"
msgstr "Roker"

#: ../../../tribes/atlantids/workers/soldier/conf:2
msgid "Soldier"
msgstr "Soldaat"

#: ../../../tribes/atlantids/workers/spiderbreeder/conf:2
msgid "Spiderbreeder"
msgstr "Spinnenkweker"

#: ../../../tribes/atlantids/workers/stonecutter/conf:2
msgid "Stonecutter"
msgstr "Steenhouwer"

#: ../../../tribes/atlantids/workers/stonecutter/conf:3
msgid "Cuts raw pieces of granit out of laying stones."
msgstr "Houwt stukken graniet uit  gesteente."

#: ../../../tribes/atlantids/workers/toolsmith/conf:2
msgid "Toolsmith"
msgstr "Werktuigsmid"

#: ../../../tribes/atlantids/workers/trainer/conf:2
msgid "Trainer"
msgstr "Trainer"

#: ../../../tribes/atlantids/workers/weaponsmith/conf:2
msgid "Weaponsmith"
msgstr "Wapensmid"

#: ../../../tribes/atlantids/workers/weaver/conf:2
msgid "Weaver"
