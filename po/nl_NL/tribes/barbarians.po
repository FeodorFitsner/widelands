#: ../../../tribes/barbarians/trunk/conf:1
msgid "Trunks are the primary build material of the barbarians."
msgstr "Boomstammen zijn het basisbouwmateriaal van de barbaren."

#: ../../../tribes/barbarians/warhelmet/conf:1
msgid "An enhanced mask for barbarian warriors."
msgstr "Een verbeterd masker voor barbarenstrijders."

#: ../../../tribes/barbarians/warmill/conf:30
#: ../../../tribes/barbarians/metalworks/conf:30
#: ../../../tribes/barbarians/axefactory/conf:26
msgid "Produce axe"
msgstr "Maak bijl"

#: ../../../tribes/barbarians/warmill/conf:31
