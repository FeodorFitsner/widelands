msgid "Fish Breeder"
msgstr "Hodowca ryb"

#: ../../../tribes/atlanteans/workers/fish_breeder/conf:3
msgid "Breeds fish"
msgstr "Hoduje ryby"

#: ../../../tribes/atlanteans/workers/fisher/conf:2
msgid "Fisher"
msgstr "Rybak"

#: ../../../tribes/atlanteans/workers/fisher/conf:3
msgid "The fisher fishes delicious fish."
msgstr "Łowi pyszne ryby."

