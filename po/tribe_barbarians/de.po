#: ../../tribes/barbarians/farm/conf:14
msgid "Plant wheat"
msgstr "Pflanzt Weizen"

#: ../../tribes/barbarians/farm/conf:16
msgid "Harvest wheat"
msgstr "Erntet Weizen"

#: ../../tribes/barbarians/farmer/conf:2
msgid "Plants fields."
msgstr "Bestellt Felder."

#: ../../tribes/barbarians/ferner/conf:1
msgid "Grows flax and thatch reed."
msgstr "Lässt Flachs und Reet wachsen."

#: ../../tribes/barbarians/fernery/conf:13
msgid "Plant flax"
msgstr "Pflanzt Flachs"

#: ../../tribes/barbarians/fernery/conf:15
