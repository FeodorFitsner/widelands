#, c-format
msgid "%1$s has left the game (%2$s)"
msgstr "Dh'fhàg %1$s an geama (%2$s)"

#: ../../src/network/network_gaming_messages.cc:134
#, c-format
msgid "Unknown user has left the game (%s)"
msgstr "Dh'fhàg cleachdaiche neo-aithnichte an geama (%s)"

