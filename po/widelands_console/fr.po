#: ../../src/wlapplication_messages.cc:40
#, fuzzy, c-format
msgid "This is Widelands Build %s"
msgstr "Voici Widelands-"

#: ../../src/wlapplication_messages.cc:43
msgid "Usage: widelands <option0>=<value0> ... <optionN>=<valueN>"
msgstr "Utilisation: widelands <option0>=<value0> ... <optionN>=<valueN>"

#: ../../src/wlapplication_messages.cc:44
msgid "Options:"
msgstr "Options:"

#: ../../src/wlapplication_messages.cc:46
msgid " --<config-entry-name>=value overwrites any config file setting"
msgstr ""
" --<config-entry-name>=value Ré-écris tous les paramètres des fichiers de "
"configuration"

#: ../../src/wlapplication_messages.cc:47
msgid ""
" --logfile=FILENAME   Log output to file FILENAME instead of \n"
"                      terminal output"
msgstr ""
" --logfile=FILENAME   Ecrire une sortie vers le fichier FILENAME plutôt que\n"
"                      dans la sortie terminal"

#: ../../src/wlapplication_messages.cc:49
msgid ""
" --datadir=DIRNAME    Use specified directory for the widelands\n"
"                      data files"
msgstr ""
" --datadir=DIRNAME    Utilise le dossier spécifié pour les données\n"
"                      de Widelands"

#: ../../src/wlapplication_messages.cc:51
msgid ""
" --homedir=DIRNAME    Use specified directory for widelands config\n"
"                      files, savegames and replays"
msgstr ""
" --homedir=DIRNAME    Utilise le dossier spécifié pour la sauvegarde\n"
"                      des fichiers de configuration de Widelands, "
"sauvegardes et replays"

#: ../../src/wlapplication_messages.cc:54
msgid "                      Default is ~/.widelands"
msgstr "                      Par défaut est ~/.widelands"

#: ../../src/wlapplication_messages.cc:57
msgid ""
" --coredump=[yes|no]  Generates a core dump on segfaults instead\n"
"                      of using the SDL"
msgstr ""
" --coredump=[yes|no]  Génère un dump du noyau sur une faute de segmentation "
"plutot\n"
"                      que d'utiliser SDL"

#: ../../src/wlapplication_messages.cc:59
msgid ""
" --language=[de_DE|sv_SE|...]\n"
"                      The locale to use."
msgstr ""
" --language=[de_DE|sv_SE|...]\n"
"                      Locales à utiliser."

#: ../../src/wlapplication_messages.cc:61
msgid " --localedir=DIRNAME  Use DIRNAME as location for the locale"
msgstr " --localedir=DIRNAME  Utilise DIRNAME comme endroit pour les locales"

#: ../../src/wlapplication_messages.cc:62
msgid ""
" --remove_syncstreams=[true|false]\n"
"                      Remove syncstream files on startup"
msgstr ""
" --remove_syncstreams=[true|false]\n"
"                      Enlève les fichiers syncstream au démarrage"

#: ../../src/wlapplication_messages.cc:64
msgid ""
" --remove_replays=[...]\n"
"                      Remove replays after this number of days.\n"
"                      If this is 0, replays are not deleted."
msgstr ""
" --remove_replays=[...]\n"
"                      Enlève le replay après ce nombre de jours.\n"
"                      Si ceci est 0, les replays ne seront pas effacés."

#: ../../src/wlapplication_messages.cc:68
msgid "Sound options:"
msgstr "Options sonores:"

#: ../../src/wlapplication_messages.cc:69
msgid " --nosound            Starts the game with sound disabled."
msgstr " --nosound            Démarre le jeu avec le son désactivé."

#: ../../src/wlapplication_messages.cc:70
msgid " --disable_fx         Disable sound effects."
msgstr " --disable_fx         Désactive les effets sonores."

#: ../../src/wlapplication_messages.cc:71
msgid " --disable_music      Disable music."
msgstr " --disable_music      Désactive la musique."

#: ../../src/wlapplication_messages.cc:72
msgid " --nozip              Do not save files as binary zip archives."
msgstr ""
" --nozip              Ne pas sauver les fichiers en archives zip binaires."

#: ../../src/wlapplication_messages.cc:73
msgid ""
" --editor             Directly starts the Widelands editor.\n"
"                      You can add a =FILENAME to directly load\n"
"                      the map FILENAME in editor."
msgstr ""
" --editor             Démarre directement l'éditeur de Widelands.\n"
"                      Vous pouvez ajouter =FILENAME pour charger "
"directement\n"
"                      la carte FILENAME dans l'éditeur."

#: ../../src/wlapplication_messages.cc:76
msgid ""
" --scenario=FILENAME  Directly starts the map FILENAME as scenario\n"
"                      map."
msgstr ""
" --scenario=FILENAME  Démarre directement la carte FILENAME comme carte de\n"
"                      scénario."

#: ../../src/wlapplication_messages.cc:78
msgid " --loadgame=FILENAME  Directly loads the savegame FILENAME."
msgstr " --loadgame=FILENAME  Charge directement la sauvegarde FILENAME."

#: ../../src/wlapplication_messages.cc:79
msgid ""
" --script=FILENAME    Run the given Lua script after initialization.\n"
"                      Only valid with --scenario, --loadgame, or --editor."
msgstr ""
" --script=FILENAME    Exécute le script LUA donné après l'initialisation.\n"
"                      Seulement valide avec --scenario, --loadgame, ou --"
"editor."

#: ../../src/wlapplication_messages.cc:81
msgid " --dedicated=FILENAME Starts a dedicated server with FILENAME as map"
msgstr ""
" --dedicated=FILENAME Démarre un serveur dédié avec FILENAME comme carte"

#: ../../src/wlapplication_messages.cc:82
msgid ""
" --auto_roadbuild_mode=[yes|no]\n"
"                      Whether to enter roadbuilding mode\n"
"                      automatically after placing a flag that is\n"
"                      not connected to a road."
msgstr ""
" --auto_roadbuild_mode=[yes|no]\n"
"                      Si vous voulez passer en mode construction de route\n"
"                      automatiquement après avoir placé un drapeau qui\n"
"                      n'est pas connecté à une route."

#: ../../src/wlapplication_messages.cc:86
msgid "Graphic options:"
msgstr "Options graphiques:"

#: ../../src/wlapplication_messages.cc:87
msgid ""
" --fullscreen=[yes|no]\n"
"                      Whether to use the whole display for the\n"
"                      game screen."
msgstr ""
" --fullscreen=[yes|no]\n"
"                      Si vous utilisez tout l'affichage pour\n"
"                      l'écran de jeu."

#: ../../src/wlapplication_messages.cc:90
msgid " --xres=[...]         Width of the window in pixel."
msgstr " --xres=[...]         Largeur de la fenêtre en pixels."

#: ../../src/wlapplication_messages.cc:91
msgid " --yres=[...]         Height of the window in pixel."
msgstr " --yres=[...]         Hauteur de la fenêtre en pixels."

#: ../../src/wlapplication_messages.cc:92
msgid " --opengl=[0|1]       Enables OpenGL rendering"
msgstr " --opengl=[0|1]       Active le rendu OpenGL"

#: ../../src/wlapplication_messages.cc:93
msgid "Options for the internal window manager:"
msgstr "Options pour le gestionnaire de fenêtre interne:"

#: ../../src/wlapplication_messages.cc:94
msgid ""
" --border_snap_distance=[0 ...]\n"
"                      Move a window to the edge of the screen\n"
"                      when the edge of the window comes within\n"
"                      this distance from the edge of the screen."
msgstr ""
" --border_snap_distance=[0 ...]\n"
"                      Bouge une fenêtre vers le bord de l'écran\n"
"                      Quand le bord de la fenêtre rentre dans\n"
"                      cette distance du bord de l'écran."

#: ../../src/wlapplication_messages.cc:98
msgid ""
" --dock_windows_to_edges=[yes|no]\n"
"                      Eliminate a window's border towards the\n"
"                      edge of the screen when the edge of the\n"
"                      window is next to the edge of the screen."
msgstr ""
" --dock_windows_to_edges=[yes|no]\n"
"                      Supprime la bordure de fenêtre vers le\n"
"                      bord de l'écran quand le bord de la\n"
"                      fenêtre est à coté du bord de l'écran."

#: ../../src/wlapplication_messages.cc:102
msgid ""
" --panel_snap_distance=[0 ...]\n"
"                      Move a window to the edge of the panel when\n"
"                      the edge of the window comes within this\n"
"                      distance from the edge of the panel."
msgstr ""
" --panel_snap_distance=[0 ...]\n"
"                      Bouge une fenêtre vers le bord du panneau quand\n"
"                      le bord de la fenêtre entre dans cette\n"
"                      distance du bord du panneau."

#: ../../src/wlapplication_messages.cc:106
msgid ""
" --snap_windows_only_when_overlapping=[yes|no]\n"
"                      Only move a window to the edge of a panel\n"
"                      if the window is overlapping with the\n"
"                      panel."
msgstr ""
" --snap_windows_only_when_overlapping=[yes|no]\n"
"                      Bouge seulement la fenêtre vers le bord du panneau\n"
"                      si la fenêtre est au dessus du\n"
"                      panneau."

#: ../../src/wlapplication_messages.cc:111
msgid " --verbose            Enable verbose debug messages"
msgstr " --verbose            Active les messages verbeux de debogage"

#: ../../src/wlapplication_messages.cc:113
msgid " --help               Show this help"
msgstr " --help               Montre cette aide"

#: ../../src/wlapplication_messages.cc:115
msgid ""
"Bug reports? Suggestions? Check out the project website:\n"
"        https://launchpad.net/widelands\n"
"\n"
"Hope you enjoy this game!"
msgstr ""
"Rapport de bug ? Suggestions ? Visitez le site web de notre projet:\n"
"        https://launchpad.net/widelands\n"
"\n"
"Nous espérons que vous avez aimez ce jeu!"

