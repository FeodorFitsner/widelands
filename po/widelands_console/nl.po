#. * TRANSLATORS: %s = version information
#: ../../src/wlapplication_messages.cc:40
#, c-format
msgid "This is Widelands Build %s"
msgstr ""

#: ../../src/wlapplication_messages.cc:43
msgid "Usage: widelands <option0>=<value0> ... <optionN>=<valueN>"
msgstr "Gebruik: widelands <optie0>=<waarde0> ... <optieN>=<waardeN>"

#: ../../src/wlapplication_messages.cc:44
msgid "Options:"
msgstr "Opties:"

#: ../../src/wlapplication_messages.cc:46
msgid " --<config-entry-name>=value overwrites any config file setting"
msgstr ""
" --<config-entry-name>=waarde overschrijft een instelling uit het "
"configuratiebestand"

#: ../../src/wlapplication_messages.cc:47
msgid ""
" --logfile=FILENAME   Log output to file FILENAME instead of \n"
"                      terminal output"
msgstr ""
" --logfile=BESTANDSNAAM log uitvoer naar bestand BESTANDSNAAM in plaats van "
"\n"
"                      scherm uitvoer"

#: ../../src/wlapplication_messages.cc:49
msgid ""
" --datadir=DIRNAME    Use specified directory for the widelands\n"
"                      data files"
msgstr ""
" --datadir=DIRNAAM Gebruik gespecificeerde directory voor Widelands\n"
"                      databestanden"

#: ../../src/wlapplication_messages.cc:51
msgid ""
" --homedir=DIRNAME    Use specified directory for widelands config\n"
"                      files, savegames and replays"
msgstr ""
" --homedir=DIRNAAM Gebruik gespecificeerde directory voor Widelands \n"
"                      configuratiebestanden, opgeslagen spellen en "
"herhalingen"

#: ../../src/wlapplication_messages.cc:54
msgid "                      Default is ~/.widelands"
msgstr "                      Standaard is ~/.widelands"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the widelands textdomain
#: ../../src/wlapplication_messages.cc:59
msgid ""
" --coredump=[true|false]\n"
"                      Generates a core dump on segfaults instead of\n"
"                      using the SDL"
msgstr ""

#: ../../src/wlapplication_messages.cc:62
msgid ""
" --language=[de_DE|sv_SE|...]\n"
"                      The locale to use."
msgstr ""
" --language=[de_DE|sv_SE|...]\n"
"                      Deze lokale gebruiken."

#: ../../src/wlapplication_messages.cc:64
msgid " --localedir=DIRNAME  Use DIRNAME as location for the locale"
msgstr " --localedir=DIRNAAM Gebruik DIRNAAM als locatie voor de lokale"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the widelands textdomain
#: ../../src/wlapplication_messages.cc:67
msgid ""
" --remove_syncstreams=[true|false]\n"
"                      Remove syncstream files on startup"
msgstr ""
" --remove_syncstreams=[true|false]\n"
"                      Verwijder syncstream bestanden bij het opstarten"

#: ../../src/wlapplication_messages.cc:69
msgid ""
" --remove_replays=[...]\n"
"                      Remove replays after this number of days.\n"
"                      If this is 0, replays are not deleted."
msgstr ""
" --remove_replays=[...]\n"
"                      Verwijder herhalingen na dit aantal dagen.\n"
"                      Als dit 0 is worden de herhalingen niet verwijderd."

#: ../../src/wlapplication_messages.cc:73
msgid "Sound options:"
msgstr "Geluidsopties:"

#: ../../src/wlapplication_messages.cc:74
msgid " --nosound            Starts the game with sound disabled."
msgstr " --nosound Start het spel zonder geluid."

#: ../../src/wlapplication_messages.cc:75
msgid " --disable_fx         Disable sound effects."
msgstr " --disable_fx Schakel geluidseffecten uit."

#: ../../src/wlapplication_messages.cc:76
msgid " --disable_music      Disable music."
msgstr " --disable_music Schakel muziek uit."

#: ../../src/wlapplication_messages.cc:77
msgid " --nozip              Do not save files as binary zip archives."
msgstr " --nozip Sla bestanden niet op als binaire zip archieven."

#: ../../src/wlapplication_messages.cc:78
msgid ""
" --editor             Directly starts the Widelands editor.\n"
"                      You can add a =FILENAME to directly load\n"
"                      the map FILENAME in editor."
msgstr ""
" --editor Start meteen de Widelands editor.\n"
"                      Je kunt =BESTANDNAAM toevoegen om direct\n"
"                      de kaart BESTANDNAAM in de editor te laden."

#: ../../src/wlapplication_messages.cc:81
msgid ""
" --scenario=FILENAME  Directly starts the map FILENAME as scenario\n"
"                      map."
msgstr ""
" --scenario=BESTANDNAAM  Start meteen de map BESTANDNAAM als scenario\n"
"                      kaart."

#: ../../src/wlapplication_messages.cc:83
msgid " --loadgame=FILENAME  Directly loads the savegame FILENAME."
msgstr " --loadgame=BESTANDNAAM Laad meteen het opgeslagen spel BESTANDNAAM."

#: ../../src/wlapplication_messages.cc:84
msgid ""
" --script=FILENAME    Run the given Lua script after initialization.\n"
"                      Only valid with --scenario, --loadgame, or --editor."
msgstr ""
" --script=BESTANDSNAAM Start het opgegeven script na initialisatie.\n"
"                      Alleen geldig met --scenario, --loadgame of --editor."

#: ../../src/wlapplication_messages.cc:86
msgid " --dedicated=FILENAME Starts a dedicated server with FILENAME as map"
msgstr " --dedicated=FILENAME Begint een vaste server met FILENAME als map"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the widelands textdomain
#: ../../src/wlapplication_messages.cc:89
msgid ""
" --auto_roadbuild_mode=[true|false]\n"
"                      Whether to enter roadbuilding mode\n"
"                      automatically after placing a flag that is\n"
"                      not connected to a road."
msgstr ""

#: ../../src/wlapplication_messages.cc:93
msgid "Graphic options:"
msgstr "Grafische opties:"

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the widelands textdomain
#: ../../src/wlapplication_messages.cc:96
msgid ""
" --fullscreen=[true|false]\n"
"                      Whether to use the whole display for the\n"
"                      game screen."
msgstr ""

#: ../../src/wlapplication_messages.cc:99
msgid " --xres=[...]         Width of the window in pixel."
msgstr " --xres=[...]         Breedte van het scherm in pixels."

#: ../../src/wlapplication_messages.cc:100
msgid " --yres=[...]         Height of the window in pixel."
msgstr " --yres=[...]         Hoogte van het scherm in pixels."

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the widelands textdomain
#: ../../src/wlapplication_messages.cc:103
msgid ""
" --opengl=[true|false]\n"
"                      Enables OpenGL rendering"
msgstr ""

#: ../../src/wlapplication_messages.cc:105
msgid "Options for the internal window manager:"
msgstr "Opties voor de interne vensterbeheerder:"

#: ../../src/wlapplication_messages.cc:106
msgid ""
" --border_snap_distance=[0 ...]\n"
"                      Move a window to the edge of the screen\n"
"                      when the edge of the window comes within\n"
"                      this distance from the edge of the screen."
msgstr ""
" --border_snap_distance=[0 ...]\n"
"                      Verplaats een venster naar de rand van het scherm\n"
"                      als de rand van het scherm binnen deze afstand van\n"
"                      de rand van het scherm komt."

#. * TRANSLATORS: You may translate true/false, also as on/off or yes/no, but
#. * TRANSLATORS: it HAS TO BE CONSISTENT with the translation in the widelands textdomain
#: ../../src/wlapplication_messages.cc:112
msgid ""
" --dock_windows_to_edges=[true|false]\n"
"                      Eliminate a window's border towards the\n"
"                      edge of the screen when the edge of the\n"
"                      window is next to the edge of the screen."
msgstr ""

#: ../../src/wlapplication_messages.cc:116
msgid ""
" --panel_snap_distance=[0 ...]\n"
"                      Move a window to the edge of the panel when\n"
"                      the edge of the window comes within this\n"
"                      distance from the edge of the panel."
msgstr ""
" --panel_snap_distance=[0 ...]\n"
"                      Verplaats een venster naar de rand van het paneel als\n"
"                      de rand van het venster binnen deze afstand van de\n"
"                      rand van het paneel komt."

#: ../../src/wlapplication_messages.cc:120
msgid ""
" --snap_windows_only_when_overlapping=[yes|no]\n"
"                      Only move a window to the edge of a panel\n"
"                      if the window is overlapping with the\n"
"                      panel."
msgstr ""
" --snap_windows_only_when_overlapping=[yes|no]\n"
"                      Verplaats alleen een venster naar de rand\n"
"                      van een paneel als het venster het paneel\n"
"                      overlapt."

#: ../../src/wlapplication_messages.cc:125
msgid " --verbose            Enable verbose debug messages"
msgstr " --verbose            Activeer uitgebreide debug-berichten"

#: ../../src/wlapplication_messages.cc:127
msgid " --help               Show this help"
msgstr " --help Toon deze hulp"

#: ../../src/wlapplication_messages.cc:129
msgid ""
"Bug reports? Suggestions? Check out the project website:\n"
"        https://launchpad.net/widelands\n"
"\n"
"Hope you enjoy this game!"
msgstr ""
"Bug meldingen? Suggesties? Bekijk de website van het project:\n"
"        https://launchpad.net/widelands\n"
"\n"
"Hopelijk vind je dit spel leuk!"

