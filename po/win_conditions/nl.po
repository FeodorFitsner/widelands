#: ../../scripting/win_conditions/02_collectors.lua:11
msgid "Collectors"
msgstr "Verzamelaars"

#: ../../scripting/win_conditions/01_defeat_all.lua:39
msgid "Congratulations!"
msgstr "Gefeliciteerd!"

#: ../../scripting/win_conditions/00_endless_game.lua:7
msgid "Endless Game"
msgstr "Eindeloos Spel"

#: ../../scripting/win_conditions/03_tribes_together.lua:85
msgid "Game over"
msgstr "Spel afgelopen"

#: ../../scripting/win_conditions/01_defeat_all.lua:28
