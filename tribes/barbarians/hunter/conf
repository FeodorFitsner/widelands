help=_The Hunter brings good meat to the colonists.
program=hunt

[buildcost]
carrier=1
hunting_spear=1

[hunt]
0=findobject type:bob radius:13 attrib:eatable # A hunter walks tremendous ways for food
1=walk object
2=animation idle 1000 # Play a hunting animation
3=object remove
4=createitem meat
5=return

