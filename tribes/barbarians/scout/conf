help=_"Scouts like Scotty the scout scout unscouted area in a scouty fashion." # (c) WiHack Team 02.01.2010
program=scout

[buildcost]
carrier=1

[scout]
0=scout 15 75000 # radius 15, 75 seconds until return
1=return

