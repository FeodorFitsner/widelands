help=_Plants trees.
program=plant

[buildcost]
shovel=1
carrier=1

[plant]
0=findspace size:any radius:5 avoid:seed
1=walk coords
2=animation idle 500 # Play a planting animation
3=plant tree1_t tree2_t tree3_t tree4_t tree5_t tree6_t tree7_t tree8_t
4=animation idle 500
5=return

[idle]
pics=idle_??.png
hotspot=5 13

[walk]
dirpics=walk_!!_??.png
hotspot=5 13

[walkload]
