##### Order does matter here, since this file should stay compatible with S2

[tundra]
def_resources=water 5
is=green
edge_texture=tundra_edge

[mountain1]
resources=coal, iron, gold, granit
is=mountain
edge_texture=mountain1_edge

[ice_flows]
is=water
fps=5
edge_texture=ice_flows_edge

[ice_flows2]
is=water
fps=5
edge_texture=ice_flows2_edge

[ice]
is=dry
#### of course ice is not dry, but this disallows the users, to build on snow. only flags are allowed. ;-)
edge_texture=ice_edge

[water]
def_resources=fish 4
is=water
fps=8
edge_texture=water_edge

